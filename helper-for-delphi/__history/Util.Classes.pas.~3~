unit Util.Classes;

interface

type
  TProessamentoNotaFiscal = class
  private
    FNumerNota: Integer;
  public
    property NumerNota: Integer read FNumerNota write FNumerNota;
    procedure Salvar();
  end;

implementation

uses
  Vcl.Dialogs;

{ TProessamentoNotaFiscal }

procedure TProessamentoNotaFiscal.Salvar;
begin
  ShowMessage('Salvando Nota Fiscal --> ' + NumerNota );
end;

end.
